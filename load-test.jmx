        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Generate test values">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.commons.lang3.RandomStringUtils

def generator = { String alphabet, int n -&gt;
  new Random().with {
    (1..n).collect { alphabet[ nextInt( alphabet.length() ) ] }.join()
  }
}

// Number of UUIDs to generate
int uuidCount = 100

// CSV file path
String csvFilePath = &quot;load-test-values.csv&quot;;

// Generate and save UUIDs to CSV
def values = (1..uuidCount).collect { value -&gt; [ uuid: UUID.randomUUID().toString(), prisonNumber: generator((&apos;A&apos;..&apos;Z&apos;).join(), 1) + generator((&apos;0&apos;..&apos;9&apos;).join(), 4) + generator((&apos;A&apos;..&apos;Z&apos;).join(), 2) ] }
new File(csvFilePath).withWriter { out -&gt;
    values.each { value -&gt; out.println(value.uuid + &apos;,&apos; + value.prisonNumber) }
}
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
